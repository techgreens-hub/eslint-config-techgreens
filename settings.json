{
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "editor.fontFamily": "'Liga Roboto Mono','Roboto Mono',Menlo",
    "editor.tabSize": 2,
    "editor.insertSpaces": false,
    "editor.formatOnSave": true,
    "javascript.implicitProjectConfig.checkJs": true,
    "emmet.includeLanguages": {
        "javascript": "javascriptreact"
    },
    "workbench.colorCustomizations": {
      "terminal.background": "#273237",
      "list.activeSelectionBackground": "#00BCD4",
      "list.inactiveFocusBackground": "#00BCD4",
      "list.dropBackground": "#00BCD4",
      "pickerGroup.foreground": "#00BCD4",
      "pickerGroup.border": "#00BCD4",
      "editorCursor.foreground": "#00BCD4",
      "editorSuggestWidget.selectedBackground": "#00BCD4",
      "tab.activeBorder": "#00BCD4",
      "activityBar.dropBackground": "#00BCD4",
      "activityBarBadge.background": "#00BCD4",
      "list.focusBackground": "#00838F", // List highlight 800
      "list.focusForeground": "#ffffff",
      "editor.findMatchBackground": "#00BCD47a", // Transparent 500
      "editorGroup.dropBackground": "#00BCD428", // Transparent 500
      "editorIndentGuide.activeBackground": "#666666"
    },
    "git.enableSmartCommit": true,
    "workbench.colorTheme": "Atom Material Theme",
    "editor.accessibilitySupport": "off",
    // "editor.wordWrap": "on",
    "editor.wrappingIndent": "indent",
    "window.zoomLevel": 0,
    "gitlens.advanced.messages": {
      "suppressShowKeyBindingsNotice": true
    },
    "files.defaultLanguage": "javascript",
    "workbench.startupEditor": "none",
    "editor.fontLigatures": true,
    "debug.openDebug": "openOnDebugBreak",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "diffEditor.ignoreTrimWhitespace": true,
    "gitlens.views.fileHistory.enabled": true,
    "gitlens.views.lineHistory.enabled": true,
    "editor.suggestSelection": "first",
    "[javascript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.detectIndentation": false,
    "liveshare.featureSet": "insiders",
    "[json]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[html]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[jsonc]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    }
  }
  